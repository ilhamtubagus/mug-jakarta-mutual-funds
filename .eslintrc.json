{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "import/extensions": "off"
    },
    "globals": {
        "expect": true
    },
    "plugins": ["jest"]
}
